{
  "name": "contacthub-connect-zendesk",
  "author": {
    "name": "Developer",
    "email": "developer@contactlab.com",
    "url": "https://github.com/contactlab"
  },
  "defaultLocale": "en",
  "private": true,
  "location": {
    "support": {
      "ticket_sidebar": "assets/iframe.html",
      "user_sidebar": "assets/iframe.html"
    }
  },
  "parameters": [
    {
      "name": "shared_secret",
      "type": "text",
      "secure": true,
      "required": true
    },
    {
      "name": "target_url",
      "type": "text",
      "required": true
    },
    {
      "name": "target_domain",
      "type": "text",
      "required": true
    }
  ],
  "domainWhitelist": ["{{setting.target_domain}}"],
  "version": "1.0",
  "frameworkVersion": "2.0"
}
